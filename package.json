{
  "name": "caccl-deploy",
  "version": "0.17.0-npm-pub-v16",
  "description": "A cli tool for managing ECS/Fargate app deployments",
  "main": "index.js",
  "bin": {
    "caccl-deploy": "index.js"
  },
  "scripts": {
