{
  "name": "caccl-deploy",
  "description": "A cli tool for managing ECS/Fargate app deployments",
  "version": "1.0.0",
  "author": "Jay Luker",
  "bin": {
    "caccl-deploy": "./bin/run.js"
  },
  "bugs": "https://github.com/harvard-edtech/caccl-deploy/issues",
  "dependencies": {
    "@aws-sdk/client-acm": "^3.777.0",
    "@aws-sdk/client-cloudformation": "^3.777.0",
    "@aws-sdk/client-ec2-instance-connect": "^3.777.0",
    "@aws-sdk/client-ecr": "^3.777.0",
    "@aws-sdk/client-ecs": "^3.778.0",
    "@aws-sdk/client-secrets-manager": "^3.777.0",
    "@aws-sdk/client-ssm": "^3.777.0",
    "@aws-sdk/client-sts": "^3.777.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "aws-cdk": "~2.1021.0",
    "aws-cdk-lib": "~2.206.0",
    "camel-case": "^4.1.2",
    "chalk": "4.1.2",
    "conf": "^10.2.0",
    "constructs": "~10.1.97",
    "env-paths": "^3.0.0",
    "figlet": "^1.5.2",
    "flat": "^5.0.2",
    "moment": "^2.29.3",
    "object-hash": "^3.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.3.7",
    "source-map-support": "^0.5.21",
    "table": "^6.8.0",
    "tempy": "^3.1.0",
    "ts-node": "^10.9.1",
    "untildify": "^4.0.0",
    "yn": "4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4.3.19",
    "@types/figlet": "^1.5.8",
    "@types/flat": "^5.0.5",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/object-hash": "^2.2.1",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "@types/tempy": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "aws-sdk-mock": "^6.1.1",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-oclif": "^5.2.1",
    "eslint-config-oclif-typescript": "^3.1.11",
    "eslint-config-prettier": "^9",
    "eslint-formatter-table": "^7.32.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cdk": "^1.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "mocha": "^10",
    "mocha-suppress-logs": "^0.5.1",
    "oclif": "^4",
    "prettier": "^2.7.1",
    "shx": "^0.3.3",
    "sinon": "^19.0.2",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "cdk.json"
  ],
  "homepage": "https://github.com/harvard-edtech/caccl-deploy",
  "keywords": [
    "DCE",
    "CLI",
    "AWS",
    "ECS"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "caccl-deploy",
    "dirname": "caccl-deploy",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "prettier --config .prettierrc.json --write --ignore-path .gitignore",
      "eslint -c .eslintrc.json --fix"
    ]
  },
  "repository": "harvard-edtech/caccl-deploy",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "shx rm -rf dist && tsc -b --watch",
    "cli": "./bin/run.js",
    "lint": "eslint -c .eslintrc.json .",
    "lint-fix": "eslint -c .eslintrc.json .  --fix",
    "format": "prettier --config .prettierrc.json --write --ignore-path .gitignore .",
    "prepare": "husky install || true",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "TS_NODE_TRANSPILE_ONLY=true TS_NODE_FILES=true mocha --require test/hooks.ts \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
